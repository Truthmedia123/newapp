  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_ENV=development&& tsx server/dev-server.ts",
    "build": "vite build && esbuild server/worker.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:production": "vite build && esbuild server/worker.ts --platform=neutral --packages=external --external:zod --external:better-sqlite3 --bundle --format=esm --outdir=dist",
    "pages:dev": "wrangler pages dev dist/public --port 8787",
    "pages:deploy": "wrangler pages deploy dist/public --project-name=weddingreplit",
    "build:optimize": "npm run optimize-images && npm run build:production",
    "optimize-images": "node scripts/optimize-images.js",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "tsx scripts/deploy-production.ts",
    "deploy:pages": "wrangler pages deploy dist/public --project-name=weddingreplit",
    "analyze": "npm run build:production -- --analyze",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "node scripts/seed-database.js",
    "import:vendors": "node scripts/import-vendors.js",
    "update-deps": "node scripts/update-dependencies.js",
    "index:vendors": "node scripts/index-vendors-meilisearch.js",
    "generate-sw": "workbox generateSW workbox-config.js",
    "dev:all": "concurrently \"npm run dev:directus\" \"npm run dev:search\" \"npm run pages:dev\"",
    "dev:directus": "cd directus-cms && npm start",
    "dev:search": "start-meilisearch.bat",
    "dev:dashboard": "tsx scripts/dev-dashboard.ts",
    "setup": "npm install && cd directus-cms && npm install",
    "install:services": "node install-services.js",
    "seed": "tsx scripts/seed-data.ts",
    "seed:reset": "tsx scripts/reset-and-seed.ts",
    "demo:enable": "DEMO_MODE=true tsx scripts/demo-mode.ts",
    "demo:reset": "tsx scripts/demo-mode.ts --reset",
    "seed:demo": "tsx scripts/demo-mode.ts",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose build",
    "docker:clean": "docker-compose down -v",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:smoke": "node scripts/production-smoke-test.js",
    "deploy:verify": "node scripts/deploy-verify.js",
    "rollback": "tsx scripts/rollback-deployment.ts",
    "test:directus": "node test-directus-connection.js"
  },
